{
	"manifest_version": 2,
	"name": "Pokemon Go CP Calculator",
	"version": "1.0.0",
	"description": "An extension that allowed user to calculate Pokemon's CP after ",
	"version": "1.0",
	"icons": { "16": "16.png",
		"48": "48.png",
		"128": "128.png"
	},
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["popup.js"]
	}],
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action" : {
		"default_popup": "popup.html",
		"default_title": "popup menu"
	},
	"permissions": ["http://www.pokego.org/*"]
}